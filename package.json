    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/simple-peer": "^9.11.5",
    "@types/three": "^0.158.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "express-validator": "^7.3.1",
    "fast-check": "^3.15.0",
