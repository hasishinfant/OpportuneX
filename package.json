    "cors": "^2.8.6",
    "ethers": "^6.9.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.5",
    "helmet": "^8.1.0",
    "lucide-react": "^0.400.0",
    "morgan": "^1.10.1",
    "next": "^15.0.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.18.0",
    "postcss": "^8.4.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.0.0",
    "redis": "^5.10.0",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "tailwind-merge": "^3.4.0",
