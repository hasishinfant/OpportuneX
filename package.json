{
  "name": "opportunex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:property": "jest --config jest.property.config.js",
    "test:smoke": "jest --config jest.smoke.config.js",
    "test:all": "npm run test && npm run test:integration && npm run test:property",
    "code-quality": "npm run lint && npm run type-check && npm run format:check",
    "code-quality:fix": "npm run lint:fix && npm run format",
    "blockchain:compile": "cd contracts && npm run compile",
    "blockchain:test": "cd contracts && npm test",
    "blockchain:deploy:local": "cd contracts && npm run deploy:local",
    "blockchain:deploy:mumbai": "cd contracts && npm run deploy:mumbai",
    "blockchain:node": "cd contracts && npm run node",
    "mvp:install": "npm install && cd server && npm install && cd ..",
    "mvp:seed": "cd server && npm run seed && cd ..",
    "mvp:backend": "cd server && npm run dev",
    "mvp:start": "./start-mvp.sh",
    "mvp:setup": "npm run mvp:install && npm run mvp:seed"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.2.0",
